https://www.codechef.com/LTIME92C/problems/EVENGAME

#include <stdio.h>
int main(void) 
{
    int t;
    scanf("%d",&t);
    while(t--)
    {
        int n;
        scanf("%d",&n);
        int a[n],i;
        for(i=0;i<n;i++)
        scanf("%d",&a[i]);
        int e=0,o=0;
        for(i=0;i<n;i++)
        {
            if(a[i]%2) o++;
            else e++;
        }
        if(o%2==0)  printf("1\n");
        else  printf("2\n");
    }
    return 0;
}

