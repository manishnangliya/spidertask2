https://www.codechef.com/LTIME92C/problems/EVENDIFF

#include <stdio.h>
int main()
{
    int t,n;
    scanf("%d",&t);
    while(t--)
    {
        scanf("%d",&n);
        int a[n],i;
        for(i=0;i<n;i++)  
           scanf("%d",&a[i]);
        int o=0,e=0;
        for(i=0;i<n;i++)  
        {
            if(a[i]%2)
              o++;
             else e++;
        }
        if(o<e)
        printf("%d\n",o);
        else  printf("%d\n",e);
    }
    return 0;
    
}

